             xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
             xmlns:tns="http://lameduck.dtu.ws/" 
             xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype"  
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
             name="LameDuck" 
             targetNamespace="http://lameduck.dtu.ws/" 
             xmlns:bs="urn://types.fastmoney.imm.dtu.dk">
<wsdl:import namespace="http://lameduck.dtu.ws/" location="LameDuck.wsdl"/>
<wsdl:binding name="LameDuckBinding" type="tns:LameDuckPortType">
        <soap:binding transport="http://schemas.xmlsoap.org/soap/http" style="rpc"/>
        <wsdl:operation name="getFlights">
            <soap:operation soapAction="getFlights_Action"/>
            <wsdl:input name="input1">
                <soap:body use="literal" namespace="http://lameduck.dtu.ws/"/>
            </wsdl:input>
            <wsdl:output name="output1">
                <soap:body use="literal" namespace="http://lameduck.dtu.ws/"/>
            </wsdl:output>
        </wsdl:operation>
        <wsdl:operation name="bookFlight">
            <soap:operation soapAction="bookFlight_Action"/>
            <wsdl:input name="input2">
                <soap:body use="literal" namespace="http://lameduck.dtu.ws/"/>
            </wsdl:input>
            <wsdl:output name="output2">
                <soap:body use="literal" namespace="http://lameduck.dtu.ws/"/>
            </wsdl:output>
            <wsdl:fault name="fault2">
                <soap:fault name="fault2"/>
            </wsdl:fault>
        </wsdl:operation>
        <wsdl:operation name="cancelFlight">
            <soap:operation soapAction="cancelFlight_Action"/>
            <wsdl:input name="input3">
                <soap:body use="literal" namespace="http://lameduck.dtu.ws/"/>
            </wsdl:input>
            <wsdl:output name="output3">
                <soap:body use="literal" namespace="http://lameduck.dtu.ws/"/>
            </wsdl:output>
            <wsdl:fault name="fault1">
                <soap:fault name="fault1"/>
            </wsdl:fault>
        </wsdl:operation>
    </wsdl:binding>
    <wsdl:service name="LameDuckService">
        <wsdl:port name="LameDuckPortType" binding="tns:LameDuckBinding">
            <soap:address location="REPLACE_WITH_ACTUAL_URL"/>
        </wsdl:port>
    </wsdl:service>
