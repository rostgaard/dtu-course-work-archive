<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:j2seproject3="http://www.netbeans.org/ns/j2se-project/3" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${libs.jaxws21.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-planningPort_5" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/planningPort_5"/>
        <property name="wsdl-planningPort_5" location="xml-resources/web-service-references/planningPort_5/wsdl/localhost_9080/planningService/planningPort.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/planningPort_5" destdir="${build.generated.dir}/jax-wsCache/planningPort_5" wsdl="${wsdl-planningPort_5}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="http://localhost:9080/planningService/planningPort?wsdl" xnocompile="true">
            <depends file="${wsdl-planningPort_5}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/planningPort_5"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/planningPort_5">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-planningPort_5" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/planningPort_5"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/ws/planning"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-planningPort_5"/>
</project>
