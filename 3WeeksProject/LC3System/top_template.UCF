#######################################
##	System clock 
##	To correctly enable the system clock you should uncoment all 4 lines that describe the clock!
#######################################
NET "clk100" LOC = "AJ15";		#Understående er blevet rettet fra clk til clk100  
NET "clk100" IOSTANDARD = LVCMOS25;
NET "clk100" TNM_NET = "clk100";
TIMESPEC "TS_clk" = PERIOD "clk100" 10.00 ns HIGH 50 %;


#***********************************************************************************#
#***********************************************************************************#
#       UCF onstraints for the DIO4 board                                           #
#                                                                                   #
#***********************************************************************************#
#***********************************************************************************#


#######################################
##	Switches on DIO4
#######################################
# Defined as a vector called SW
NET "sw_i<0>"	LOC = "N6";	
NET "sw_i<1>"	LOC = "L5";
NET "sw_i<2>"	LOC = "M2";
NET "sw_i<3>"	LOC = "P9";
NET "sw_i<4>"	LOC = "M4";
NET "sw_i<5>"	LOC = "N1";
NET "sw_i<6>"	LOC = "P8";
NET "sw_i<7>"	LOC = "N4";

NET "sw_i<*>" IOSTANDARD = LVTTL;

#######################################
#	Buttons on DIO4
#######################################
# Defined as a vector called BTN
NET "btn_i<0>"	LOC = "P3";
NET "btn_i<1>"	LOC = "P2";
NET "btn_i<2>"	LOC = "W3";
NET "btn_i<3>"	LOC = "V8";
NET "btn_i<4>"	LOC = "AA1";
NET "btn_i<*>" IOSTANDARD = LVTTL;


#######################################
#	LEDS on DIO4
#######################################
# Defined as a vector called LED
# and enable bit called LEDG
NET "led<0>"	LOC = "T8";
NET "led<1>"	LOC = "U5";
NET "led<2>"	LOC = "W2";
NET "led<3>"	LOC = "U9";
NET "led<4>"	LOC = "V4";
NET "led<5>"	LOC = "Y1";
NET "led<6>"	LOC = "U8";
NET "led<7>"	LOC = "V6";

NET "ledg"	LOC = "V7";

NET "led*" IOSTANDARD = LVTTL;


#######################################
#	7 Segment on DIO4
#######################################
NET "seg<6>"	LOC = "N5";	# A #MHI Understående er blevet rettet ved at flytte dem en plads så de passer til vores display og ikke edgars.
NET "seg<5>"	LOC = "L4";	# B
NET "seg<4>"	LOC = "N2";	# C
NET "seg<3>"	LOC = "R9";	# D
NET "seg<2>"	LOC = "M3";	# E
NET "seg<1>"	LOC = "P1";	# F
NET "seg<0>"	LOC = "P7";	# G
NET "seg<7>"	LOC = "N3"; # Dot
NET "seg<*>" IOSTANDARD = LVTTL;
#
NET "an<3>"	LOC = "W4";
NET "an<2>"	LOC = "W5";
NET "an<1>"	LOC = "AB1";
NET "an<0>"	LOC = "Y4";

NET "an<*>" IOSTANDARD = LVTTL;






